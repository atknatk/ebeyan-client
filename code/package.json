    "ng2-pdf-viewer": "^3.0.2",
    "ngx-bootstrap": "^1.7.1",
    "ngx-loading": "^1.0.5",
    "ngx-popover": "^0.0.16",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.0.1",
    "scriptjs": "^2.5.8",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.7",

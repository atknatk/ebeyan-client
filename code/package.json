    "imports-loader": "^0.7.0",
    "inputmask": "^3.3.6",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "ncp": "^2.0.0",
    "protractor": "~4.0.13",
    "raw-loader": "0.5.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
